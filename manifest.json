{
"manifest_version": 2,
"name": "Ui Video Tools",
"version": "0.1",
"description": "Adds an expand button to the unifi video interface",
"content_scripts": [
    {
        "matches": [
            "https://video.ui.com/*",
            "https://*:7443/*"
        ],
        "js": ["content.js"]
    }
],
"browser_action": {
    "default_icon": "icon.png"
},
"background": {
    "scripts": ["background.js"]
},
"homepage_url": "https://github.com/littlej247/Unifi-Video-Tools"
}
